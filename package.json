{
  "name": "newdefi",
  "version": "1.0.0",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "install": "npx husky install",
    "private-network": "ganache-cli --time 1970-01-01T00:00:00Z -e 1000000 -l 10000000",
    "private-network-fork": "ganache-cli -e 1000000 -f",
    "compile": "truffle compile",
    "compile-abi": "truffle-abi",
    "compile-types": "typechain --target=web3-v1 --out-dir=build/web3-types build/abi/*.json",
    "compile-web": "npm run compile && npm run compile-abi && npm run compile-types",
    "test": "truffle test --compile-all --migrations_directory test",
    "test-fork": "truffle test ./forkTest/* --compile-all --migrations_directory test",
    "test-all": "npm run deploy-dev && truffle test --compile-none --migrations_directory test",
    "deploy-rinkeby": "truffle deploy --network rinkeby --reset --compile-all",
    "deploy-dev": "truffle deploy --network development --reset --compile-all",
    "lint-sol": "solhint \"contracts/**/*.sol\"",
    "lint-fix": "npm run lint-js-fix && npm run lint-sol-fix",
    "lint-js-fix": "prettier --write \"./**/*.js\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@chainlink/contracts": "^0.2.0",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.0",
    "@truffle/hdwallet-provider": "^1.4.0",
    "@uniswap/v3-core": "^1.0.0",
    "@uniswap/v3-periphery": "^1.1.1",
    "bignumber.js": "^9.0.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "^6.12.2",
    "truffle": "^5.1.49",
    "web3": "1.2.11"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eth-gas-reporter": "^0.2.22",
    "mocha": "^8.1.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "*",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.1",
    "truffle-abi": "^1.0.3",
    "truffle-assertions": "^0.9.2",
    "truffle-flattener": "^1.5.0",
    "truffle-plugin-verify": "^0.5.7",
    "truffle-test-utils": "^0.1.0",
    "typechain": "^5.0.0"
  }
}
