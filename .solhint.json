{
  "extends": "solhint:recommended",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "reason-string": [
      "error",
      {
        "maxLength": 100
      }
    ],
    "reentrancy": "off",
    "prettier/prettier": "off",
    "modifier-name-mixedcase": "off",
    "no-empty-blocks": "off",
    "func-visibility": "off",
    "max-states-count": "off",
    "not-rely-on-time": "off",
    "compiler-version": "off"
  }
}